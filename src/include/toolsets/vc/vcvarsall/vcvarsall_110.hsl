        hefesto.sys.setenv("PATH", $aux);
        $aux = hefesto.sys.env("INCLUDE");
        $aux = hefesto.sys.make_path($temp, "include\\shared") + ";" + hefesto.sys.make_path($temp, "include\\um") + ";" + hefesto.sys.make_path($temp, "include\\winrt") + ";" + $aux;
        hefesto.sys.setenv("INCLUDE", $aux);
        $aux = hefesto.sys.env("LIB");
        $aux = hefesto.sys.make_path($temp, "lib\\win8\\um\\x64") + ";" + $aux;
        hefesto.sys.setenv("LIB", $aux);
        $aux = hefesto.sys.env("LIBPATH");
        $aux2 = hefesto.sys.env("ExtensionSDKDir");
        $aux = hefesto.sys.make_path($temp, "References\\CommonConfiguration\\Neutral") + ";" + hefesto.sys.make_path($aux2, "\\Microsoft.VCLibs\\11.0\\References\\CommonConfiguration\\neutral") + ";" + $aux;
        hefesto.sys.setenv("LIBPATH", $aux);
    }

    $temp = hefesto.sys.env("VSINSTALLDIR");
    $temp = hefesto.sys.make_path($temp, "Team Tools\\Performance Tools\\x64");
