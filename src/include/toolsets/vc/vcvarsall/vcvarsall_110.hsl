#
# "vcvarsall_110.hsl"
#       by Rafael Santiago
#
# Description: this file sets up the environment variables
#              which are relevants to cl.exe works. This is
#              a port of "vcvarsall.bat".
#
# The option --cpu-arch specifies which platform vc compiler
# must be configured. The possible values are:
#       x86 or 32
#       amd64, x64 or 64
#       arm
#       x86_amd64
#       x86_arm
#       or empty (attempts to detect the machine architecture).
#

include ~/toolsets/vc/common/vssetuputils.hsl

function vc110_vcvarsall(hwtype type string) : result type int
{
    var exit_code type int;
    var already_done type string;
    $already_done = hefesto.sys.env("HFSTVS110VCVARSALLWASHERE");
    if ($already_done == "1") {
        result 0;
    }
    $exit_code = 1;
    if ($hwtype.len() == 0) {
        $hwtype = get_cpu_architecture();
    }
    if ($hwtype == "x86" || $hwtype == "32") {
        $exit_code = vs110_vcvars32_x86();
    } else if ($hwtype == "amd64" || $hwtype == "x64" || $hwtype == "64") {
        $exit_code = vs110_vcvars64_amd64();
    } else if ($hwtype == "arm") {
        $exit_code = vs110_vcvars_arm();
    } else if ($hwtype == "x86_arm") {
        $exit_code = vs110_vcvars_x86_arm();
    } else if ($hwtype == "x86_amd64") {
        $exit_code = vs110_vcvars_x86_amd64();
    } else {
        hefesto.sys.echo(hefesto.project.toolset() + " fatal error: unknown processor architecture: \"" + $hwtype + "\". ");
        hefesto.sys.echo("CLUE: possible values to --cpu-arch option should be: x86 | amd64 | arm | x86_amd64 | x86_arm | or empty.\n");
    }
    if ($exit_code == 0) {
        $exit_code = set_visual_studio_version("11.0");
    }
    if ($exit_code == 0) {
        hefesto.sys.setenv("HFSTVS110VCVARSALLWASHERE", "1");
    }
    result $exit_code;
}

function set_visual_studio_version(version type string) : result type int
{
    result hefesto.sys.setenv("VisualStudioVersion", $version);
}

function vs110_vcvars64_amd64() : result type int
{

    var temp type string;
    var mtype type list;

    $temp = hefesto.sys.env("VS110COMNTOOLS");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the location of the VS Common Tools folder.\n");
        result 1;
    }

    $mtype.add_item("No32bit");
    $mtype.add_item("64bit");

    vcvars_query_registry($mtype);

    $temp = hefesto.sys.env("VSINSTALLDIR");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the location of the VS installation.\n");
        result 1;
    }

    $temp = hefesto.sys.env("VCINSTALLDIR");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the location of the VC installation.\n");
        result 1;
    }

    $temp = hefesto.sys.env("FrameworkDir64");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the location of the .NET Framework 64bit installation.\n");
        result 1;
    }
    hefesto.sys.setenv("FrameworkDir", $temp);

    $temp = hefesto.sys.env("FrameworkVersion64");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the version of the .NET Framework 64bit installation.\n");
        result 1;
    }
    hefesto.sys.setenv("FrameworkVersion", $temp);

    $temp = hefesto.sys.env("Framework35Version");
    if ($temp.len() == 0) {
        hefesto.sys.echo("ERROR: Cannot determine the .NET Framework 3.5 version.\n");
        result 1;
    }

    var aux type string;
    var aux2 type string;

    $temp = hefesto.sys.env("WindowsSdkDir_old");
    if ($temp.len() > 0) {
        $temp = hefesto.sys.env("PATH");
        $aux = hefesto.sys.env("WindowsSdkDir_35");
        $temp = hefesto.sys.make_path($temp, "bin\\NETFX 4.0 Tools\\x64") + ";" + $aux + "x64" + ";" + hefesto.sys.make_path($aux, "bin\\NETFX 4.0 Tools") + ";" + $aux + ";" + $temp;
        hefesto.sys.setenv("PATH", $temp);
    }

    $temp = hefesto.sys.env("WindowsSdkDir");
