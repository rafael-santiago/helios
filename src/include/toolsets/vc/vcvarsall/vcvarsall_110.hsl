    if (vspath_exists($temp) == 1) {
        $aux = $temp + ";" + hefesto.sys.env("LIBPATH");
        hefesto.sys.setenv("LIBPATH", $aux);
    }
    $temp = hefesto.sys.env("VCINSTALLDIR");
